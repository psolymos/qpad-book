@article{ WalkerJSS2012,
Author = {Walker, Steven C. and Guenard, Guillaume and Solymos, Peter and
   Legendre, Pierre},
Title = {{Multiple-Table Data in R with the multitable Package}},
Journal = {{JOURNAL OF STATISTICAL SOFTWARE}},
Year = {{2012}},
Volume = {{51}},
Number = {{8}},
Pages = {{1-38}},
Month = {{NOV}},
Abstract = {{Data frames are integral to R. They provide a standard format for
   passing data to model-fitting and plotting functions, and this standard
   makes it easier for experienced users to learn new functions that accept
   data as a single data frame. Still, many data sets do not easily fit
   into a single data frame; data sets in ecology with a so-called
   fourth-corner problem provide important examples. Manipulating such
   inherently multiple-table data using several data frames can result in
   long and difficult-to-read workflows. We introduce the R multitable
   package to provide new data storage objects called data.list
   objects,which extend the data.frame concept to explicitly multiple-table
   settings. Like data frames, data lists are lists of variables stored as
   vectors; what is new is that these vectors have dimension attributes
   that make accessing and manipulating them easier. As data.list objects
   can be coerced to data.frame objects, they can be used with all R
   functions that accept an object that is coercible to a data.frame.}},
